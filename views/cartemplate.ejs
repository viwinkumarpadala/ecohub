<!DOCTYPE html>
<html>

<head>
    <!-- css -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.css">
    <!-- css -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.1/owl.carousel.js"></script>
    <link rel="stylesheet" href="/Cartemplatestyle.css">
    <link rel="shortcut icon" href="/Animated Leaf Icon final.jpg" type="image/x-icon">
    <title><%= car.title %> </title>
</head>

<body>
    <!-- car detail start -->
    <section id="car-detail">
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">


        <div class="card">
            <div class="card-body">
                <h1 class="card-title class"><%= car.title %> </h1> <big></big> <big></big>
                <h5 class="card-subtitle"></h5>
                <div class="row">
                    <div class="white-box textC"> <br></br>
                        <img src="<%= car.img_link %> ">

                    </div>
                    <!-- <div class='container2'></div> -->
                    <div class="col-lg-7 col-md-7 col-sm-6 content">
                        <h4 class="box-title mt-5">Car description</h4>
                        <p><%= car.desc %> </p>
                        <br>
                        <!-- <h2 class="mt-5">
                         $153<small class="text-success">(36%off)</small>
                     </h2> -->
                        <!-- <button class="btn btn-dark btn-rounded mr-1" data-toggle="tooltip" title="" data-original-title="Add to cart">
                         <i class="fa fa-shopping-cart"></i>
                     </button> -->
                        <a href="#contactform"><button class="btn btn-primary btn-rounded">Book Now</button></a>
                        <h3 class="box-title mt-5">Key Highlights</h3>
                        <ul class="list-unstyled">
                            <li><i class="fa fa-check text-success"></i>Good Performance</li>
                            <li><i class="fa fa-check text-success"></i>Premium Interiors</li>
                            <li><i class="fa fa-check text-success"></i>Better Engine</li>
                        </ul>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <h3 class="box-title mt-5">General Info</h3>
                        <div class="table-responsive">
                            <table class="table table-striped table-product">
                                <tbody>
                                    <tr>
                                        <td width="390">Brand</td>
                                        <td><%= car.brand %> </td>
                                    </tr>
                                    <tr>
                                        <td>Model</td>
                                        <td><%= car.model %></td>
                                    </tr>
                                    <tr>
                                        <td>Variant</td>
                                        <td><%= car.variant %></td>
                                    </tr>
                                    <tr>
                                        <td>Body Type</td>
                                        <td>CSUV</td>
                                    </tr>
                                    <tr>
                                        <td>Year</td>
                                        <td>2019</td>
                                    </tr>
                                    <tr>
                                        <td>Owners</td>
                                        <td>1</td>
                                    </tr>
                                    <tr>
                                        <td>Condition</td>
                                        <td>Good</td>
                                    </tr>
                                    <tr>
                                        <td>Loan</td>
                                        <td>No</td>
                                    </tr>
                                    <tr>
                                        <td>Transmission</td>
                                        <td>MT</td>
                                    </tr>
                                    <tr>
                                        <td>State</td>
                                        <td>MH</td>
                                    </tr>
                                    <tr>
                                        <td>Accesories</td>
                                        <td>NA</td>
                                    </tr>
                                    <tr>
                                        <td>Warranty</td>
                                        <td>3Y</td>
                                    </tr>
                                    <tr>
                                        <td>Tyres</td>
                                        <td>Good Condition</td>
                                    </tr>
                                    <tr>
                                        <td>Color</td>
                                        <td>Grey</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </section>
    <!-- car detail end -->
</body>

</html>